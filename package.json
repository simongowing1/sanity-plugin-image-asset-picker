{
  "name": "sanity-plugin-image-asset-picker",
  "version": "1.0.0",
  "description": "Enables user to bulk-select image assets to add to image array.",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "sanityPlugin": {
    "verifyPackage": {
      "tsc": false
    }
  },
  "homepage": "https://github.com/simongowing1/sanity-plugin-image-asset-picker#readme",
  "bugs": {
    "url": "https://github.com/simongowing1/sanity-plugin-image-asset-picker/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/simongowing1/sanity-plugin-image-asset-picker.git"
  },
  "license": "MIT",
  "author": "Simon Gowing <simongowing1@gmail.com>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict",
    "prepare": "husky"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/ui": "^2.15.7",
    "phosphor-react": "^1.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@sanity/pkg-utils": "^7.1.1",
    "@sanity/plugin-kit": "^4.0.19",
    "@sanity/semantic-release-preset": "^5.0.0",
    "@types/react": "^19.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "date-fns": "^4.1.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sanity": "^3.80.1",
    "styled-components": "^6.1.16",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=18"
  }
}
